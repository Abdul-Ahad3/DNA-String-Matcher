{
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }, 
    { "src": "../api/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/match", "dest": "../api/server.js" },
    { "src": "/dfa-dot", "dest": "../api/server.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
